---
import Layout from '../layouts/Layout.astro'
---

<Layout
  title='Page not found'
  pageClass='page__not_found'
  showHeader={false}
  showFooter={false}
  showISI={false}
>
  <section class='design-system-section'>
    <div class='section-wrapper section-wrapper--small'>
      <h1 class='title'>Tabs 1</h1>
      <ul role='tablist'>
        <li role='presentation'>
          <!--active one -->
          <a href='#section1' role='tab' id='tab1' aria-selected='true'>Tab 1</a
          >
        </li>
        <li role='presentation'>
          <a href='#section2' role='tab' id='tab2' tabindex='-1'>Tab 2</a>
        </li>
        <li role='presentation'>
          <a href='#section3' role='tab' id='tab3' tabindex='-1'>Tab 3</a>
        </li>
        <li role='presentation'>
          <a href='#section4' role='tab' id='tab4' tabindex='-1'>Tab 4</a>
        </li>
        <li role='presentation'>
          <a href='#section5' role='tab' id='tab5' tabindex='-1'>Tab 5</a>
        </li>
      </ul>
      <section
        id='section1'
        role='tabpanel'
        aria-labelledby='tab1'
        aria-hidden='false'
        tabindex='-1'
      >
        <h2>Panel 1 - 1</h2>
      </section>
      <section
        id='section2'
        role='tabpanel'
        aria-labelledby='tab2'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 1 - 2</h2>
      </section>
      <section
        id='section3'
        role='tabpanel'
        aria-labelledby='tab3'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 1 - 3</h2>
      </section>
      <section
        id='section4'
        role='tabpanel'
        aria-labelledby='tab4'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 1 - 4</h2>
      </section>
      <section
        id='section5'
        role='tabpanel'
        aria-labelledby='tab5'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 1 - 5</h2>
      </section>
    </div>

    <div class='section-wrapper section-wrapper--small'>
      <h1 class='title'>Tabs 2</h1>
      <ul role='tablist' data-horizontal-scroll>
        <li role='presentation'>
          <!--active one -->
          <a href='#panel1' role='tab' id='tab1' aria-selected='true'>Tab 1</a>
        </li>
        <li role='presentation'>
          <a href='#panel2' role='tab' id='tab2' tabindex='-1'>Tab 2</a>
        </li>
        <li role='presentation'>
          <a href='#panel3' role='tab' id='tab3' tabindex='-1'>Tab 3</a>
        </li>
        <li role='presentation'>
          <a href='#panel4' role='tab' id='tab4' tabindex='-1'>Tab 4</a>
        </li>
        <li role='presentation'>
          <a href='#panel5' role='tab' id='tab5' tabindex='-1'>Tab 5</a>
        </li>
        <li role='presentation'>
          <a href='#panel6' role='tab' id='tab6' tabindex='-1'>Tab 6</a>
        </li>
        <li role='presentation'>
          <a href='#panel7' role='tab' id='tab7' tabindex='-1'>Tab 7</a>
        </li>
      </ul>
      <section
        id='panel1'
        role='tabpanel'
        aria-labelledby='tab1'
        aria-hidden='false'
        tabindex='-1'
      >
        <h2>Panel 1</h2>

        <div class='section'>
          <ul role='tablist'>
            <li role='presentation'>
              <a
                href='#sub-panel1'
                role='tab'
                id='sub-tab1'
                aria-selected='true'>Sub Tab 1</a
              >
            </li>
            <li role='presentation'>
              <a href='#sub-panel2' role='tab' id='sub-tab2' tabindex='-1'
                >Sub Tab 2</a
              >
            </li>
            <li role='presentation'>
              <a href='#sub-panel3' role='tab' id='sub-tab3' tabindex='-1'
                >Sub Tab 3</a
              >
            </li>
          </ul>

          <section
            id='sub-panel1'
            role='tabpanel'
            aria-labelledby='sub-tab1'
            aria-hidden='false'
            tabindex='-1'
          >
            <h2>Sub Panel 1</h2>
          </section>

          <section
            id='sub-panel2'
            role='tabpanel'
            aria-labelledby='sub-tab2'
            aria-hidden='true'
            tabindex='-1'
            hidden
          >
            <h2>Sub Panel 2</h2>
          </section>

          <section
            id='sub-panel3'
            role='tabpanel'
            aria-labelledby='sub-tab3'
            aria-hidden='true'
            tabindex='-1'
            hidden
          >
            <h2>Sub Panel 3</h2>
          </section>
        </div>
      </section>
      <section
        id='panel2'
        role='tabpanel'
        aria-labelledby='tab2'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 2</h2>
      </section>
      <section
        id='panel3'
        role='tabpanel'
        aria-labelledby='tab3'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 3</h2>
      </section>
      <section
        id='panel4'
        role='tabpanel'
        aria-labelledby='tab4'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 4</h2>
      </section>
      <section
        id='panel5'
        role='tabpanel'
        aria-labelledby='tab5'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 5</h2>
      </section>
      <section
        id='panel6'
        role='tabpanel'
        aria-labelledby='tab6'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 6</h2>
      </section>
      <section
        id='panel7'
        role='tabpanel'
        aria-labelledby='tab7'
        aria-hidden='true'
        tabindex='-1'
        hidden
      >
        <h2>Panel 7</h2>
      </section>
    </div>
  </section>
</Layout>
